                                        "Ref": "AWS::Region"
                                    },
                                    " --no-paginate --query 'LoadBalancerDescriptions[?Scheme==`internal`].[LoadBalancerName]' --output text) ; do\n",
                                    "    LB=$(aws elb describe-tags --load-balancer-names ${lb} --region ",
                                        {
                                            "Ref": "AWS::Region"
                                        },
                                        " --query 'TagDescriptions[? Tags[? Value==`",
                                            {
                                                "Ref": "AWS::StackId"
                                            },
                                            "` && Key==`aws:cloudformation:stack-id`] && Tags[? Value==`OpenShiftMasterInternalELB` && Key==`aws:cloudformation:logical-id`]].[LoadBalancerName]' --output text)\n",
                                            "    if [ \"$(echo $LB)\" != \"\" ] ; then\n",
                                            "        aws elb delete-load-balancer-listeners --load-balancer-name ${LB} --load-balancer-ports 443 --region ",
                                            {
                                                "Ref": "AWS::Region"
                                            },
                                            "\n",
                                            "sleep $sleep\n",
                                            "        aws elb create-load-balancer-listeners --load-balancer-name ${LB} --listeners Protocol=tcp,LoadBalancerPort=443,InstanceProtocol=tcp,InstancePort=443 --region ",
                                            {
                                                "Ref": "AWS::Region"
                                            },
                                            "\n",
                                            "    fi\n",
                                            "done\n",
                                            "sleep $sleep ; let \"sleep+=5\"; fi ; done\n",
                                            "echo \"[INFO] Finished OpenShift Cluster Build\" \n",
                                            {
                                                "Fn::If": [
                                                    "EnableAWSServiceBroker",
                                                    {
                                                        "Fn::Join": [
                                                            "",
                                                            [
                                                                "echo \"[INFO] Installing the AWS Service Broker\" \n",
                                                                "AWSSB_SETUP_HOST=$(cat /etc/ansible/hosts | awk NF | grep -A1 '\\[masters\\]' | tail -n 1 | awk '{print $1}')\n",
                                                                "ssh $AWSSB_SETUP_HOST 'SCRIPT_PATH=",
                                                                {
                                                                    "Fn::Sub": "s3://${QSS3BucketName}/${QSS3KeyPrefix}"
                                                                },
                                                                " ; ",
                                                                "yum install -y wget && ",
                                                                "mkdir -p ~/aws_broker_install && ",
                                                                "cd ~/aws_broker_install && ",
                                                                "wget https://s3.amazonaws.com/awsservicebroker/scripts/deploy-awsservicebroker.template.yaml && ",
                                                                "wget https://s3.amazonaws.com/awsservicebroker/scripts/deploy_aws_broker.sh && ",
                                                                "chmod +x deploy_aws_broker.sh && ",
                                                                "./deploy_aws_broker.sh && ",
                                                                "aws s3 cp ${SCRIPT_PATH}scripts/secrets.yaml ./secrets.yaml && ",
                                                                "sed -i \"s~<CFN_ROLE_ARN>~",
                                                                {
                                                                    "Fn::If": [
                                                                        "CreateAWSServiceBrokerRole",
                                                                        {
                                                                            "Fn::GetAtt": [
                                                                                "AWSServiceBrokerCFNRole",
                                                                                "Arn"
                                                                            ]
                                                                        },
                                                                        {
                                                                            "Ref": "ExistingAWSServiceBrokerRole"
                                                                        }
                                                                    ]
                                                                },
                                                                "~\" ./secrets.yaml &&",
                                                                "sed -i \"s/<REGION>/",
                                                                {
                                                                    "Ref": "AWS::Region"
                                                                },
                                                                "/\" ./secrets.yaml && ",
                                                                "sed -i \"s/<VPC_ID>/",
                                                                {
                                                                    "Ref": "VPCID"
                                                                },
                                                                "/\" ./secrets.yaml && ",
                                                                "oc create -f ./secrets.yaml -n aws-service-broker && ",
                                                                "oc get configmap broker-config -n aws-service-broker -o yaml > aws-sb-config.yaml && ",
                                                                "sed -i \"s/^kind: ConfigMap$/    secrets:\\n&/\" aws-sb-config.yaml && ",
                                                                "for apb in $(echo \"dh-sqs-apb dh-sns-apb dh-r53-apb dh-rds-apb dh-emr-apb dh-redshift-apb dh-elasticache-apb dh-dynamodb-apb dh-s3-apb dh-athena-apb\"); do ",
                                                                "  sed -i \"s/^kind: ConfigMap$/      - {apb_name: $apb, secret: aws-secret, title: aws-secret}\\n&/\" aws-sb-config.yaml ; ",
                                                                "done ; ",
                                                                "oc replace -f ./aws-sb-config.yaml -n aws-service-broker && ",
                                                                "oc rollout status dc/aws-asb  -n aws-service-broker ; oc rollout latest aws-asb -n aws-service-broker' || qs_err \" failed to setup the AWS Service Broker\"\n",
                                                                "echo \"[INFO] Finished Installing the AWS Service Broker\" \n"
                                                            ]
                                                        ]
                                                    },
                                                    ""
                                                ]
                                            },
                                            "echo \"[INFO] Signaling Stack .....\"\n",
                                            "# Signal resource using [qs_status] via cfn-init\n",
                                            "cfn-signal -e $(qs_status)",
                                            "   --stack ",
                                            {
                                                "Ref": "AWS::StackName"
                                            },
                                            "   --resource AnsibleConfigServer ",
                                            "   --region ",
                                            {
                                                "Ref": "AWS::Region"
                                            },
                                            "\n",
                                            "echo \"End cfn stack signaling\"\n",
                                            "echo \"\t#################[] End of main execution block \" \n",
                                            "else\n",
                                            "echo \" \t#################[] Start of else block \" \n",
                                            "echo \"[REASON] Failed to Acquire OpenShift Entitlement, Check your PoolID and RHN UserName/Password \" >~/failure_reason\n",
                                            "echo \"[INFO] Signaling Stack .....\"\n",
                                            "cfn-signal -e 1",
                                            "   --stack ",
                                            {
                                                "Ref": "AWS::StackName"
                                            },
                                            "   --resource AnsibleConfigServer ",
                                            "   --region ",
                                            {
                                                "Ref": "AWS::Region"
                                            },
                                            "\n",
                                            "echo \" \t#################[] End of else block \" \n",
                                            "fi\n"
                            ]
                        ]
                    }
                }
            },
            "CreationPolicy": {
                "ResourceSignal": {
                    "Timeout": "PT240M"
                }
            }
        },
        "OpenShiftScalingCWEventRule": {
            "Type": "AWS::Events::Rule",
            "Properties": {
                "Description": "CWEvent rule to trigger OpenShift Scaling scripts within the Ansible config server. (AWS QuickStart)",
                "Name": "OpenshiftQuickstart-TriggerAnsibleScaling",
                "State": "ENABLED",
                "EventPattern": {
                    "source": [
                        "aws.autoscaling"
